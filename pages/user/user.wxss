page {
  background-color: rgba(232, 232, 232, 0.5);
}
.user_info_wrap {
  height: 15vh;
  margin-bottom: 20rpx;
  border-top: 1px solid rgba(232, 232, 232, 0.5);
}
.user_info_wrap .userInfo {
  height: 9vh;
  display: flex;
  margin-top: 5vh;
}
.user_info_wrap .userInfo .userIcon {
  width: 9vh;
  border-radius: 50%;
  margin: 0 20rpx;
}
.user_info_wrap .userInfo .userName {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-weight: bold;
}
.user_info_wrap .btn_userInfo {
  height: 13vh;
  margin: 15rpx;
  background-color: white;
  border-radius: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.user_info_wrap .btn_userInfo navigator {
  width: 250rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  background-color: var(--themeColor);
  color: #ffffff;
  border-radius: 35rpx;
}
.user_content .usermain .history_wrap {
  display: flex;
  margin: 15rpx;
  background-color: white;
  border-radius: 15rpx;
  padding: 15rpx;
}
.user_content .usermain .history_wrap navigator {
  flex: 1;
  text-align: center;
  font-size: 26rpx;
}
.user_content .usermain .history_wrap navigator .his_num {
  color: var(--themeColor);
}
.user_content .usermain .orders_wrap {
  background-color: white;
  border-radius: 15rpx;
  margin: 0 15rpx;
}
.user_content .usermain .orders_wrap .orders_title {
  border-bottom: 1rpx solid #cccccc;
  padding: 15rpx;
  padding-left: 25rpx;
}
.user_content .usermain .orders_wrap .orders_content {
  display: flex;
  padding: 20rpx 0;
}
.user_content .usermain .orders_wrap .orders_content navigator {
  flex: 1;
  text-align: center;
  margin: 15rpx 0;
}
.user_content .usermain .orders_wrap .orders_content navigator .iconfont {
  font-size: 40rpx;
  color: var(--themeColor);
  margin-bottom: 10rpx;
}
.user_content .usermain .orders_wrap .orders_content navigator .orders_name {
  font-size: 20rpx;
}
.user_content .usermain .address_wrap {
  background-color: white;
  border-radius: 15rpx;
  margin: 15rpx;
  padding: 30rpx;
}
.user_content .usermain .app_info_wrap {
  background-color: white;
  border-radius: 15rpx;
  margin: 0 15rpx;
}
.user_content .usermain .app_info_wrap view {
  padding: 30rpx;
}
.user_content .usermain .app_info_wrap view:nth-child(1) {
  position: relative;
}
.user_content .usermain .app_info_wrap view:nth-child(1) button {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.user_content .usermain .app_info_wrap view:nth-child(2) {
  border-bottom: 1px solid #cccccc;
  border-top: 1px solid #cccccc;
  position: relative;
}
.user_content .usermain .app_info_wrap view:nth-child(2) button {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.user_content .usermain .Share_wrap {
  background-color: white;
  border-radius: 15rpx;
  margin: 15rpx;
  padding: 30rpx;
  position: relative;
}
.user_content .usermain .Share_wrap button {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
