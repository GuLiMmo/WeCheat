.tabs .tabs_title {
  background-color: #ffffff;
  display: flex;
  padding: 15rpx 0;
  padding-bottom: 0;
}
.tabs .tabs_title .tabs_item {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  padding: 15rpx 0;
}
.tabs .tabs_title .active {
  color: var(--themeColor);
  border-bottom: 5rpx solid currentColor;
}
